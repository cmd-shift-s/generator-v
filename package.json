{
  "name": "generator-v",
  "version": "1.0.0",
  "description": "yeoman generator for vue.js",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:base test/specs/**/*.js",
    "test:base": "mocha -R spec --compilers js:babel-register",
    "test:watch": "npm run test:base -- -w test/specs/**/*.js --watch-extensions es6",
    "build": "babel generators/*/index.es6 -d .",
    "build:watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gongzza/generator-v.git"
  },
  "keywords": [
    "yeoman-generator",
    "generator-v"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gongzza/generator-v/issues"
  },
  "homepage": "https://github.com/gongzza/generator-v#readme",
  "dependencies": {
    "yeoman-generator": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "inquirer": "^2.0.0",
    "mocha": "^3.2.0",
    "rx": "^4.1.0",
    "yeoman-assert": "^2.2.2",
    "yeoman-test": "^1.6.0"
  }
}
